<div class="page-header">
  <%= link_to interviews_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_interview_path(@interview), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show interview</h1>
</div>

<dl class="dl-horizontal">
  <dt>Held on:</dt>
  <dd><%= @interview.held_on %></dd>

  <dt>Name:</dt>
  <dd><%= @interview.name %></dd>

  <dt>Company:</dt>
  <dd><%= @interview.company %></dd>

  <dt>User:</dt>
  <dd><%= @interview.user %></dd>

  <dt>Persona:</dt>
  <dd><%= @interview.persona %></dd>

  <dt>Notes:</dt>
  <dd><%= @interview.notes %></dd>

</dl>
